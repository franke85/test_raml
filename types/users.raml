#%RAML 1.0 Library

uses:
  address: address.raml

types:
  users:
    type: object
    properties:
      firstName:
        type: string
        required: false
      lastName:
        type: string
        required: false
      phone:
        type: string
        required: false
      age:
        type: string
        required: false
      userStatus:
        type: string
        required: false
     address:
       type: address.address
